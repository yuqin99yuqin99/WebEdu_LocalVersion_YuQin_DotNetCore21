<p role="status">Current count: @CounterPartialClass.CurrentCount.ToString()</p>
<button class="btn btn-primary" @onclick="CounterPartialClass.IncrementCount">Click me</button>

<p role="status">Current html: @CounterPartialClass.MyHTML</p>
<button class="btn btn-primary" @onclick="CounterPartialClass.ToggleHTML">Click me html</button>

<p role="status">Current raw html: @CounterPartialClass.MyRawHTML</p>
<button class="btn btn-primary" @onclick="CounterPartialClass.ToggleRawHTML">Click me Rawhtml</button>
<p role="status">Current raw html: @CounterPartialClass.MyHTMLContent</p>
<button class="btn btn-primary" @onclick="CounterPartialClass.ToggleMyHTMLContent">Click me HTMLContent</button>
